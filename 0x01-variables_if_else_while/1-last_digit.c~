#include <stdlib.h>
#include <time.h>
/* more headers goes there */

/* betty style doc for function main goes there */
int main(void)
{
int n;

 srand(time(0));
 n = rand() - RAND_MAX / 2;
 /* your code goes there */
 if (n%10 > 5)
   printf("and is greater than 5\n", n)
   else if((n % 10) < 6 && (n & 10) !=0 )
     printf("%d and is less than 6 and not 0 \n")
     else
       printf("and is 0");
 return (0);
}
